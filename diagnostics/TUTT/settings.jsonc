    "lon" : {
      "standard_name" : "longitude",
      "units" : "degrees_east"
    },
    "time" : {
      "standard_name" : "time",
      "units" : "mon",
    }
  },
  "varlist" : {
    "Z200": {
      // "var_name" : "Z200_var",
      "path_variable": "Z200_FILE",
      "standard_name" : "geopotential_height_200_mbar",
      "freq" : "mon",
      "requirement" : "required",
      "units" : "m",
      "dimensions" : ["time", "lat", "lon"]
    }
  }
