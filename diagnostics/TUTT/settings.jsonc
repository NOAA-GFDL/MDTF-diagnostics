{
  "settings" : {
    "long_name" : "Tropical Upper Tropospheric Trough Diagnostic Package",
    "driver" : "TUTT.py",
    "realm" : "atmos",
    "description" : "Comparing daily atmospheric circulation pattern characteristics in reanalysis and models",
    "runtime_requirements" : {
      "python3" : ["netCDF4", "xarray", "numpy", "sklearn", "cartopy", "matplotlib", "numba", "datetime", "typing"]
